page {
  background-image: url("data:image/jpg;base64,");
  color: #945414;
  height: 100%;
}

view, image, p, label, input, textarea, button, scroll-view {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

input, textarea {
  color: #333;
  font-size: 28rpx;
  background: #fefefe;
  border: 2rpx solid #ccc;
  padding: 20rpx 15rpx;
  border-radius: 5rpx;
}

input[disabled], textarea[disabled] {
  background: #e6e6e6;
}

